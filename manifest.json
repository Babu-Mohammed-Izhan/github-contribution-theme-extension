{
  "name": "GitHub Contribution Color Graph",
  "description": "Change colors of contribution graph in GitHub",
  "manifest_version": 3,
  "version": "1.7.2",

  "content_scripts": [
    {
      "js": ["contentscript.js"],
      "matches": ["https://github.com/*"],
      "run_at": "document_end"
    }
  ],
  "permissions": ["storage"],
  "action": {
    "default_popup": "options.html",
    "default_icon": {
      "16": "/images/github.png",
      "32": "/images/github.png",
      "48": "/images/github.png",
      "128": "/images/github.png"
    }
  },
  "icons": {
    "16": "/images/github.png",
    "32": "/images/github.png",
    "48": "/images/github.png",
    "128": "/images/github.png"
  }
}
